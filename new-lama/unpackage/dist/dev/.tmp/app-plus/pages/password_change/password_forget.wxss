.uni-padding-wrap {
  width: 100%;
  padding: 0;
}
.uni-padding-wrap .static {

  padding-top: var(--status-bar-height);
  background: #ff4965;

}
.uni-padding-wrap .from-logo {
  width: 100%;
}
.uni-padding-wrap .from-logo image {
  width: 100%;
}
.uni-padding-wrap .phone_ma {
  margin: 30rpx 0;
  margin: 0 10%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.uni-padding-wrap .phone_ma .mobile-code {
  width: 70%;
  border-radius: 30rpx;
  padding: 20rpx 0px;
  height: 50rpx;
  border: 2rpx solid #ccc;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
}
.uni-padding-wrap .phone_ma .mobile-code .uni-input {
  width: 100%;
  margin-left: 5%;
  padding: 0;
}
.uni-padding-wrap .phone_ma .mobile-code .login-icon {
  font-size: 46rpx;
  color: #ccc;
  z-index: 10;
  margin-left: 5%;
  line-height: 52rpx;
}
.uni-padding-wrap .phone_ma .get {
  color: #fff;
  width: 26%;
  font-size: 24rpx;
  text-align: center;
  padding: 0px;
  height: 85rpx;
  line-height: 85rpx;
  background: #FF4965;
  border-radius: 10rpx;
}
.from-column {
  width: 100%;
  background: #fff;
}
.from-column .uni-form-item {
  width: 80%;
  margin: 30rpx 0;
  margin-left: 10%;
  padding: 20rpx 0px;
  border-radius: 30rpx;
  height: 50rpx;
  border: 2rpx solid #ccc;
}
.from-column .uni-form-item .login-icon {
  font-size: 46rpx;
  color: #ccc;
  z-index: 10;
  margin-left: 3%;
  line-height: 52rpx;
}
.from-column .uni-form-item .uni-input {
  width: 80%;
  margin-left: 2%;
  padding: 0;
}
.uni-btn-v {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  width: 100%;
  text-align: center;
}
button {
  margin: 20rpx 0rpx;
  background: #ff4965;
  color: #fff;
  width: 80%;
}

