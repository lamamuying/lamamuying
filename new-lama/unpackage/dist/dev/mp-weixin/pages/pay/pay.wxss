.attr-pop-box {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 8;
  bottom: 0;
  /* display: none; */
}
.attr-pop {
  width: 92%;
  padding: 4%;
  background: #fff;
  position: fixed;
  z-index: 9;
  bottom: 0rpx;
  bottom: calc(0rpx + constant(safe-area-inset-bottom));
  bottom: calc(0rpx + env(safe-area-inset-bottom));
  overflow-y: auto;
}
.attr-pop .close {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  right: 20rpx;
  overflow: hidden;
  top: 31.25rpx;
}
.attr-pop .close .icon {
  width: 48rpx;
  height: 48rpx;
}
