.uni-padding-wrap {
  width: 100%;
  padding: 0;
}
.uni-padding-wrap .static {

}
.uni-padding-wrap .from-logo {
  width: 100%;
}
.uni-padding-wrap .from-logo image {
  width: 100%;
}
.from-column {
  width: 100%;
  padding-top: 30rpx;
  background: #fff;
}
.from-column .uni-form-item {
  width: 80%;
  margin: 30rpx 0;
  margin-left: 10%;
  padding: 20rpx 0px;
  border-radius: 30rpx;
  height: 50rpx;
  border: 2rpx solid #ccc;
}
.from-column .uni-form-item .login-icon {
  font-size: 46rpx;
  color: #ccc;
  z-index: 10;
  margin-left: 3%;
  line-height: 52rpx;
}
.from-column .uni-form-item .uni-input {
  width: 80%;
  margin-left: 2%;
  padding: 0;
}
.uni-btn-v {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  width: 100%;
  text-align: center;
}
button {
  margin: 20rpx 0rpx;
  background: #ff4965;
  color: #fff;
  width: 80%;
}

